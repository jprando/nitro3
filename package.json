{
  "name": "prando-nitro-app",
  "private": true,
  "scripts": {
    "dev": "pnpm nitro:dev",
    "nitro:dev": "nitro dev",
    "cloudflare:dev": "pnpm dlx wrangler dev",
    "check": "pnpm exec biome check --write",
    "lint": "pnpm exec biome lint --write",
    "format": "pnpm exec biome format --write",
    "build": "nitro build",
    "preview": "npx srvx --prod .output/",
    "prepreview": "pnpm build",
    "deploy": "pnpm cloduflare:deploy",
    "cloduflare:deploy": "pnpm dlx wrangler deploy",
    "cloudflare:login": "pnpm dlx wrangler login",
    "prepare": "husky install && nitro prepare"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "husky": "^9.1.4",
    "nitro": "latest",
    "nitro-cloudflare-dev": "^0.2.2",
    "wrangler": "^4.45.0"
  },
  "volta": {
    "node": "22.21.0",
    "npm": "11.6.2"
  },
  "engines": {
    "node": "22.21.0",
    "npm": "11.6.2"
  },
  "packageManager": "pnpm@10.19.0"
}
